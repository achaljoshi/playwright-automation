{
  
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "playwright"
    ],
      "env": {
        "node": true
      },
    "extends": [
      "eslint:recommended",
      "plugin:playwright/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-var-requires": "off",
      "semi": ["error", "always"]
    }
  }
  